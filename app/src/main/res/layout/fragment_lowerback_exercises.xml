<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.zActualExercises.DeltsExercisesFragment">
    <!--tools:context=".ui.MuscleGroups.MuscleGroupsFragment"!-->



    <ScrollView
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/Prisoner_Back_Extensions"
                android:layout_width="200dp"
                android:layout_height="75dp"
                android:layout_gravity="center_horizontal"
                android:layout_margin="8dp"
                android:backgroundTint="#A42A04"
                android:text="@string/Prisoner_Back_Extensions"
                android:textColor="#000000"
                android:textSize="18sp" />

            <Button
                android:id="@+id/Deadlifts"
                android:layout_width="200dp"
                android:layout_height="75dp"
                android:layout_gravity="center_horizontal"
                android:layout_margin="8dp"
                android:backgroundTint="#A42A04"
                android:text="@string/Deadlift"
                android:textColor="#000000"
                android:textSize="18sp"
                tools:ignore="TextSizeCheck" />

            <Button
                android:id="@+id/Good_Mornings"
                android:layout_width="200dp"
                android:layout_height="75dp"
                android:layout_gravity="center_horizontal"
                android:layout_margin="8dp"
                android:backgroundTint="#A42A04"
                android:text="@string/Good_Mornings"
                android:textColor="#000000"
                android:textSize="18sp"
                tools:ignore="TextSizeCheck" />

            <Button
                android:id="@+id/Kettlebell_Swings"
                android:layout_width="200dp"
                android:layout_height="75dp"
                android:layout_gravity="center_horizontal"
                android:layout_margin="8dp"
                android:backgroundTint="#A42A04"
                android:text="@string/Kettlebell_Swings"
                android:textColor="#000000"
                android:textSize="18sp"
                tools:ignore="TextSizeCheck" />

        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>